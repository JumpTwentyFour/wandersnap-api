{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/horizon": "^5.15",
        "laravel/pennant": "^1.2",
        "laravel/sanctum": "^3.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-path-prefixing": "^3.0",
        "predis/predis": "^1.1",
        "thecodingmachine/safe": "^2.5",
        "tpetry/laravel-query-expressions": "^0.5.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "brianium/paratest": "^7.1",
        "fakerphp/faker": "^1.9.1",
        "jumptwentyfour/laravel-coding-standards": "^2.2",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "php-parallel-lint/php-parallel-lint": "^1.0",
        "phpstan/extension-installer": "^1.3",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-ray": "^1.32",
        "thecodingmachine/phpstan-safe-rule": "^1.2",
        "timacdonald/log-fake": "^2.0",
        "tomasvotruba/cognitive-complexity": "^0.1.1",
        "worksome/request-factories": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": [
            "vendor/bin/parallel-lint ./ --blame --exclude vendor .phpstorm.meta.php"
        ],
        "lint:fix": [
            "vendor/bin/phpcbf --standard=./phpcs.xml --extensions=php --ignore=*.blade.php,database/*,vendor/*,.phpstorm.meta.php"
        ],
        "stan": [
            "vendor/bin/phpstan analyse --memory-limit=4G"
        ],
        "phpunit": [
            "vendor/bin/pest"
        ],
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "php artisan cache:clear",
            "php artisan test"
        ],
        "test:pipeline": [
            "Composer\\Config::disableProcessTimeout",
            "php artisan cache:clear",
            "php artisan test"
        ],
        "test:parallel": [
            "Composer\\Config::disableProcessTimeout",
            "php artisan cache:clear",
            "php artisan test --parallel"
        ],
        "ecs": [
            "vendor/bin/ecs check --debug"
        ],
        "ecs-fix": [
            "vendor/bin/ecs check --fix"
        ],
        "generate-meta": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
